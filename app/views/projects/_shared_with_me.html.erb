<div class="col-sm">
  <h6 class="top">
    <img width="20px" src="https://img.icons8.com/ios-filled/50/000000/group-foreground-selected.png"/>
    Shared with me
  </h6><br>
  <% if @shared_with_me_projects.present? %>
    <% @shared_with_me_projects.each do |project| %>
      <%= render partial: 'projects/project_card', locals: { project: project } %>
    <% end %>
  <% else %>
    <p>No projects shared with you.</p>
  <% end %>
</div>
